function initMap(lati, longi) {
  var dumbo = { lat: -25.344, lng: 131.036 };

  var mapOptions = {
    center: dumbo,
    zoom: 3,
  };
  var googlemap = new google.maps.Map(
    document.getElementById("EmployeePic").insertRow(1),
    mapOptions
  );
}


